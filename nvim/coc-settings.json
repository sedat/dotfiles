{
  "prettier.tslintIntegration": true,
  "prettier.eslintIntegration": true,
  "diagnostic.virtualText": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescript.tsx",
    "javascript",
    "java",
    "json",
    "javascriptreact",
    "go"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
